                            </content>
                        </TitledPane>
                        <TitledPane expanded="false" text="Freeholders">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <fx:include source="FreeholdersPane.fxml" fx:id="freeholdersPane" />
                                </AnchorPane>
                            </content>
                        </TitledPane>
                    </panes>
                </Accordion>
                <CheckBox layoutY="615.0" mnemonicParsing="false" selected="true" text="CheckBox" />
                <CheckBox layoutX="100.0" layoutY="615.0" mnemonicParsing="false" selected="true" text="CheckBox" />
                <CheckBox layoutX="200.0" layoutY="615.0" mnemonicParsing="false" selected="true" text="CheckBox" />
