        <AnchorPane layoutX="650.0" prefHeight="650.0" prefWidth="320.0">
            <children>
                <Accordion prefHeight="600.0" prefWidth="325.0">
                    <panes>
                        <TitledPane text="Add entity">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <fx:include source="AddEntityPane.fxml" fx:id="addEntityPane" />
                                </AnchorPane>
                            </content>
                        </TitledPane>
                        <TitledPane expanded="false" text="Entity modification">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <fx:include source="EntityModificationPane.fxml" fx:id="entityModificationPane" />
                                </AnchorPane>
                            </content>
                        </TitledPane>
                        <TitledPane expanded="false" text="Multimedia">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <fx:include source="MultimediaPane.fxml" fx:id="muldimediaPane" />
                                </AnchorPane>
